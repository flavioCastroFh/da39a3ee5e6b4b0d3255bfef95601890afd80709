                    placeholder="Email"
                    required-message="'Please enter your email'"
                    invalid-message="'You have entered an invalid email'"
                    ng-change="trackEvent('Login.Email.EmailAddress','EmailAddress')"
                    validate-on="submit">
            </div>
        </div>
        <div class="form-group" ng-show="isEmailMode()">
            <label for="inputPassword3" class="col-sm-3 control-label">Password</label>

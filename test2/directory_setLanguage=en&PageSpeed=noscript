                        <input id="facebook-login-type" type="radio" ng-checked="isGoogleMode()" ng-click="setGoogleMode()" name="type" value="2">
                        <img src="https://cdns.englishcentral.com/images/googleplus-small.png" />
                        <span>Google</span>
                    </label>
                                <label class="radio-inline">
                    <input id="email-login-type" type="radio" ng-checked="isEmailMode()" ng-click="setEmailMode()" name="type" value="1">
                    <span class="fa fa-envelope-o"></span>
                    <span>Email</span>
                </label>
            </div>
        </div>
        
        <div class="form-group" ng-show="isEmailMode()">

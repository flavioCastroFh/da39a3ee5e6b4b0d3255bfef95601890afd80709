                    class="btn btn-grey"
                    aria-hidden="true"
                    ng-click="cancelSessionModal.cancel(cancelSessionModal.params);">Yes</button>

            <button type="button"
                    ng-show="!cancelSessionModal.hasError()"
                    class="btn btn-ec" aria-hidden="true"
                    ng-click="cancelSessionModal.close();">No</button>


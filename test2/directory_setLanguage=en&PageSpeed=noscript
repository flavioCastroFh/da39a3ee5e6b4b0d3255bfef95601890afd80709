        novalidate=""
        angular-validator
        angular-validator-submit="login()">
        <div class="alert bg-danger" ng-show="hasError()">
            <span ng-show="emailError">
                Sorry, we could not match that email and password.            </span>
            <span ng-show="fbError">
                Your facebook account is not yet registered.            </span>

            <span ng-show="googleError">
                Unable to login your Google account.            </span>

            <span ng-show="validateOrganizationError">
                Account not belongs to organization            </span>
        </div>

                <div class="form-group">
